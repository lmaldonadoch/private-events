<h1>Sign up</h1>

  <%= form_for @user do |f| %>
  <ul>
 <% @user.errors.full_messages.each do |error| %>
   <li><%= error %></li>
 <% end %>
   <%= f.label :name, 'Your Uername here' %> <br>
  <%= f.text_field :name %><br>
  <%= f.label :email,'Your Email here' %> <br>
  <%= f.text_field :email %> <br><br>
  <%= f.submit "Create my account"%>
    <% end %>
  